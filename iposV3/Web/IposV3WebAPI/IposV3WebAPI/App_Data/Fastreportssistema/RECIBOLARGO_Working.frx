    private void GroupHeader1_BeforeLayout(object sender, EventArgs e)
    {
      if(Engine.FirstPass &amp;&amp; !Page1.Watermark.Enabled)
      {                                                                                                                             
        if((((Int64)Report.GetColumnValue(&quot;REC_DETAIL.lineaid&quot;))== 400) ||
           ( (
          ((Int64)Report.GetColumnValue(&quot;REC_MASTER.tipodoctoid&quot;)) == 21 ||   
          ((Int64)Report.GetColumnValue(&quot;REC_MASTER.tipodoctoid&quot;)) == 31 
             ) //&amp;&amp;                                                                   
            //((String)Report.GetColumnValue(&quot;REC_MASTER.movilcontado&quot;)) == &quot;S&quot;
          )
          )
        {     
          Page1.Watermark.Enabled = true;     
        }
      }  
    }
    
    
     
