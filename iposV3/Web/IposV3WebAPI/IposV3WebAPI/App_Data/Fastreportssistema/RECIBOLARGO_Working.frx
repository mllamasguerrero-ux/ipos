      catch(Exception ex)
      {
        Data1.Visible = false;
      }   
    }
                      

    private void PageFooter1_BeforePrint(object sender, EventArgs e)
    { 
              
      
       //para obtener el numero total de paginas.. si es la primera pasada vamos simplementa aumentando el numero de paginas
      if(Engine.FirstPass)
      {
        totalPagesFirstPass++; 
      }
      else  // si el la 2da entonces aqui ya sabes el numero de pag de la primera pasada y si debemos aumentar 1
      {
        totalPages =  totalPagesFirstPass + (paginaExtra ? 1 : 0  );
      }
      
      if(sender == PageFooter1)
      {
        //en la primera pasada determinamos si debemos agregar o no una pagina al final
        if(Engine.FirstPass)
        {
          //en cada pagina vamos viendo si fuera la ultima ¿se requeriria una nueva pagina para el total?
          if(( Engine.CurY  + PageFooter1.Height + Child1.Height + Child6.Height + Page1.BottomMargin + 2 &lt;=  Engine.PageHeight ))
          {
            paginaExtra = false;    
            //MessageBox.Show(&quot;false &quot; + Engine.PageHeight + &quot; -- &quot; +  Engine.CurY.ToString() + &quot; - &quot; + PageFooter1.Height.ToString() + &quot; - &quot; + Child1.Height.ToString());
          }
          else
          {   
              paginaExtra = true;
              //MessageBox.Show(&quot;true &quot; + Engine.PageHeight + &quot; -- &quot; + Engine.CurY.ToString() + &quot; - &quot; + PageFooter1.Height.ToString() + &quot; - &quot; + Child1.Height.ToString());
          }
          
          
        } 
          
        if(Engine.TotalPages == Engine.CurPage + 1 )
        {                                           
          if( Engine.CurY  + PageFooter1.Height + Child1.Height + Child6.Height + Page1.BottomMargin + 2 &lt;=  Engine.PageHeight )
          {    
                                                                                                      
            PageFooter1.Visible = true;
            Child1.Visible = true;  
          }
          else
          {                      
            PageFooter1.Visible = false;
            Child1.Visible = false;                    
          }                   
        
        }
        else
        {                                  
          PageFooter1.Visible = false;  
          Child1.Visible = false;
        
        } 
      }  
      
                  
          
    }   

    private void PageHeader1_BeforePrint(object sender, EventArgs e)
    {          
                                                                                                                   
      
      try
      {                       
        
        if(Engine.CurPage == 0 &amp;&amp; ((Int64)Report.GetColumnValue(&quot;REC_MASTER.tipodoctoid&quot;)) == 21 &amp;&amp; ((Int64)Report.GetColumnValue(&quot;REC_MASTER.personaapartadoid&quot;)) != 0)
        {                       
          Child2.Visible = true;
        }
        else
        {                      
          Child2.Visible = false;
        }
      }
      catch(Exception emx){            
          Child2.Visible = false;
      }                                                                 
      
      
     }
                   
     
                                
                         
                      

