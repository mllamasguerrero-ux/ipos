  public class ReportScript
  {
           
    public String MANEJORECETA = &quot;S&quot;;
    public String SOLORETENIDAS = &quot;N&quot;;    
    public String MANEJORECETASTRING = &quot;CON RECETA&quot;;
    public String SOLORETENIDASSTRING = &quot;NO&quot;;
    
    

    private void CBMANEJORECETA_TextChanged(object sender, EventArgs e)
    {
      
      string strValue = CBMANEJORECETA.Text;
      MANEJORECETASTRING = strValue;
      if(strValue.Trim().Equals(&quot;TODAS&quot;))
      {
            MANEJORECETA = &quot;T&quot;;
      }       
      else if(strValue.Trim().Equals(&quot;CON RECETA&quot;))
      {
             MANEJORECETA = &quot;S&quot;;
      }         
      else if(strValue.Trim().Equals(&quot;OMITIO RECETA&quot;))
      {       
            MANEJORECETA = &quot;R&quot;;
      }       
      else if(strValue.Trim().Equals(&quot;NO NECESITA RECETA&quot;))
      {    
            MANEJORECETA = &quot;N&quot;;
      } 
      else
      {      
            MANEJORECETA = &quot;T&quot;;
      }
    }

    private void CBSOLORETENIDAS_TextChanged(object sender, EventArgs e)
    {
        
      string strValue = CBSOLORETENIDAS.Text;
      SOLORETENIDASSTRING = strValue  ;
      if(strValue.Trim().Equals(&quot;SI&quot;))
      {
        SOLORETENIDAS = &quot;S&quot;;                    
      }       
      else 
      {
        SOLORETENIDAS = &quot;N&quot;;      
      } 
    }
  }
}
