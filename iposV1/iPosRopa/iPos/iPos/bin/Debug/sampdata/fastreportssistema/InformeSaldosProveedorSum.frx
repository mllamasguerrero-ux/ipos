        <CommandParameter Name="solovencidos" DataType="16" Expression="[solovencidos]" DefaultValue="S"/>
        <CommandParameter Name="proveedorid" DataType="1" Expression="[proveedorid]" DefaultValue="0"/>
      </TableDataSource>
