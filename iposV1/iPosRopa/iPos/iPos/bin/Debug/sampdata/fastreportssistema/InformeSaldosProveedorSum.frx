      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="select &#13;&#10;persona.clave clavepersona,&#13;&#10;persona.nombre nombrepersona,&#13;&#10;SUM(coalesce(docto.importe,0)) importe,&#13;&#10;SUM(coalesce(docto.descuento,0)) descuento,&#13;&#10;SUM(coalesce(docto.subtotal,0)) subtotal,&#13;&#10;SUM(coalesce(docto.iva,0)) iva,&#13;&#10;SUM(coalesce(docto.ieps,0)) ieps,&#13;&#10;SUM(coalesce(docto.impuesto,0)) impuesto,&#13;&#10;SUM(coalesce(docto.total,0)) total,&#13;&#10;SUM(coalesce(docto.saldo,0)) saldo&#13;&#10;from docto&#13;&#10;left join persona on persona.id = docto.personaid&#13;&#10;where &#13;&#10;docto.tipodoctoid = 11 and docto.estatusdoctoid = 1&#13;&#10;and docto.saldo &gt; 0&#13;&#10;and ( docto.vence &lt; current_date or @solovencidos &lt;&gt; 'S')&#13;&#10;and ( docto.personaid = @proveedorid or @proveedorid = 0)&#13;&#10;group by clavepersona, nombrepersona&#13;&#10;&#13;&#10;">
