      <TableDataSource Name="Table1" Alias="vendedor" DataType="System.Int32" Enabled="true" SelectCommand="select 0 as id, '' as clave, 'Todos # Vendedor: 0' as nombre from RDB$DATABASE&#13;&#10;union&#13;&#10;select id, clave, nombre ||  '  # Vendedor: ' || cast(id as varchar(13)) nombre from persona where tipopersonaid  in (22)&#13;&#10;">
        <Column Name="ID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="select d.personaid,pr.clave,  pr.nombre,&#13;&#10;sum( case when dateadd(coalesce(pr.dias,0) day to d.vence) &lt;= current_date then coalesce(d.saldo,0) else 0 end) saldovencido  ,&#13;&#10;sum( case when dateadd(coalesce(pr.dias,0) day to d.vence) &gt; current_date then coalesce(d.saldo,0) else 0 end) saldonovencido,&#13;&#10;&#13;&#10;sum(  case when datediff (day,  coalesce(dateadd(coalesce(pr.dias,0) day to d.vence), current_date),current_date) between 0 and 15 then d.saldo else 0 end) s0_15 ,&#13;&#10;sum(  case when datediff (day,  coalesce(dateadd(coalesce(pr.dias,0) day to d.vence), current_date),current_date) between 16 and 30 then d.saldo else 0 end) s16_30,&#13;&#10;sum(  case when datediff (day,  coalesce(dateadd(coalesce(pr.dias,0) day to d.vence), current_date),current_date) between 31 and 45 then d.saldo else 0 end) s31_45,&#13;&#10;sum(  case when datediff (day,  coalesce(dateadd(coalesce(pr.dias,0) day to d.vence), current_date),current_date) between 46 and 60 then d.saldo else 0 end) s46_60,&#13;&#10;sum(  case when datediff (day,  coalesce(dateadd(coalesce(pr.dias,0) day to d.vence), current_date),current_date) &gt; 61  then d.saldo else 0 end) s61mas,&#13;&#10;&#13;&#10;vendedor.clave clavevendedor,&#13;&#10;vendedor.nombre nombrevendedor&#13;&#10;&#13;&#10;&#13;&#10;from docto d&#13;&#10;inner join persona pr on d.personaid = pr.id&#13;&#10;left  join persona vendedor on pr.vendedorid = vendedor.id&#13;&#10;where d.tipodoctoid in (21,24)&#13;&#10;and coalesce(d.saldo,0) &gt; 0 and d.estatusdoctoid = 1&#13;&#10;and (@usuarioid = 0 or @usuarioid = pr.vendedorid) and coalesce(d.subtipodoctoid,0) not in (7,8)&#13;&#10;group by d.personaid  , pr.clave,pr.nombre,&#13;&#10;vendedor.clave ,&#13;&#10;vendedor.nombre&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;">
        <Column Name="PERSONAID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
        <Column Name="SALDOVENCIDO" DataType="System.Decimal"/>
        <Column Name="SALDONOVENCIDO" DataType="System.Decimal"/>
        <Column Name="S0_15" DataType="System.Decimal"/>
        <Column Name="S16_30" DataType="System.Decimal"/>
        <Column Name="S31_45" DataType="System.Decimal"/>
        <Column Name="S46_60" DataType="System.Decimal"/>
        <Column Name="S61MAS" DataType="System.Decimal"/>
        <Column Name="CLAVEVENDEDOR" DataType="System.String"/>
        <Column Name="NOMBREVENDEDOR" DataType="System.String"/>
        <CommandParameter Name="usuarioid" DataType="1" Expression="vendedorId" DefaultValue="0"/>
      </TableDataSource>
    </FirebirdDataConnection>
    <Parameter Name="US_ID" DataType="System.Decimal"/>
    <Parameter Name="US_NAME" DataType="System.String"/>
    <Parameter Name="vendedorid" DataType="System.Decimal"/>
