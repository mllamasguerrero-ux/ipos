        <Column Name="CAJEROORIGCLAVE" DataType="System.String"/>
        <Column Name="CAJEROORIGNOMBRE" DataType="System.String"/>
        <Column Name="FECHAORIGINAL" DataType="System.DateTime"/>
        <Column Name="IMPORTE" DataType="System.Decimal"/>
        <Column Name="FORMAPAGOID" DataType="System.Int64"/>
        <Column Name="FORMAPAGOCLAVE" DataType="System.String"/>
        <Column Name="FORMAPAGONOMBRE" DataType="System.String"/>
        <Column Name="NOTAS" DataType="System.String"/>
        <Column Name="BANCONOMBRE" DataType="System.String"/>
        <Column Name="REFERENCIABANCARIA" DataType="System.String"/>
        <Column Name="TIPODOCTONOMBRE" DataType="System.String"/>
        <CommandParameter Name="fechainicio" DataType="5" Expression="[fechainicio]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="fechafin" DataType="5" Expression="[fechafin]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="usuarioid" DataType="1" Expression="vendedorId" DefaultValue="0"/>
      </TableDataSource>
