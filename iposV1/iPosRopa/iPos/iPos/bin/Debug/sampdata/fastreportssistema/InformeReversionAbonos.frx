      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;DOCTOPAGO.ID,&#13;&#10;DOCTOPAGO.FECHA,&#13;&#10;DOCTO.FOLIO FOLIOVENTA,&#13;&#10;DOCTOPAGO.PAGOID FOLIOPAGO,&#13;&#10;CASE WHEN COALESCE(DOCTO.TIMBRADOSELLOCFDI,'') &lt;&gt; '' THEN&#13;&#10;        CAST(COALESCE(DOCTO.FOLIOSAT,0) AS VARCHAR(30)) || COALESCE(DOCTO.SERIESAT,'')&#13;&#10;    ELSE ''&#13;&#10;    END  FOLIOSAT,&#13;&#10;CLIENTE.CLAVE CLIENTECLAVE,&#13;&#10;CLIENTE.NOMBRE CLIENTENOMBRE,&#13;&#10;COALESCE(CLIENTE.NOMBRES,'') || ' ' || COALESCE(CLIENTE.APELLIDOS,'')  CLIENTENOMBRES,&#13;&#10;CAJEROORIG.CLAVE CAJEROORIGCLAVE,&#13;&#10;CAJEROORIG.NOMBRE CAJEROORIGNOMBRE,&#13;&#10;DPORIG.FECHA FECHAORIGINAL,    &#13;&#10;CAJERO.CLAVE CAJEROCLAVE,&#13;&#10;CAJERO.NOMBRE CAJERONOMBRE,&#13;&#10;DOCTOPAGO.IMPORTE,&#13;&#10;DOCTOPAGO.FORMAPAGOID,&#13;&#10;FORMAPAGO.CLAVE FORMAPAGOCLAVE,&#13;&#10;FORMAPAGO.NOMBRE FORMAPAGONOMBRE,&#13;&#10;DOCTOPAGO.NOTAS ,&#13;&#10;BANCOS.NOMBRE BANCONOMBRE,&#13;&#10;DOCTOPAGO.REFERENCIABANCARIA,&#13;&#10;TIPODOCTO.NOMBRE TIPODOCTONOMBRE&#13;&#10;&#13;&#10;FROM DOCTOPAGO&#13;&#10;INNER JOIN DOCTO ON DOCTO.ID = DOCTOPAGO.DOCTOID&#13;&#10;LEFT JOIN PERSONA CLIENTE ON CLIENTE.ID = DOCTO.PERSONAID&#13;&#10;LEFT JOIN CORTE ON CORTE.ID = DOCTOPAGO.CORTEID&#13;&#10;LEFT JOIN PERSONA CAJERO ON CAJERO.ID = CORTE.CAJEROID&#13;&#10;LEFT JOIN FORMAPAGO ON FORMAPAGO.ID = DOCTOPAGO.FORMAPAGOID&#13;&#10;LEFT JOIN DOCTOPAGO DPORIG ON DPORIG.ID = DOCTOPAGO.DOCTOPAGOREFID&#13;&#10;LEFT JOIN CORTE CORTEORIG ON CORTEORIG.ID = DPORIG.CORTEID&#13;&#10;LEFT JOIN PERSONA CAJEROORIG ON CAJEROORIG.ID = CORTEORIG.CAJEROID&#13;&#10;LEFT JOIN BANCOS ON BANCOS.ID = DOCTOPAGO.BANCO&#13;&#10;LEFT JOIN TIPODOCTO ON TIPODOCTO.ID = DOCTO.TIPODOCTOID&#13;&#10;WHERE  doctopago.fecha &gt;= @fechainicio and doctopago.fecha &lt;= @fechafin&#13;&#10;and doctopago.tipoabonoid = 3&#13;&#10;and (@usuarioid = 0 or @usuarioid =  cajero.id)&#13;&#10;ORDER BY DOCTOPAGO.FECHA&#13;&#10;">
        <Column Name="FOLIOPAGO" DataType="System.Int64"/>
        <Column Name="CLIENTECLAVE" DataType="System.String"/>
        <Column Name="CLIENTENOMBRE" DataType="System.String"/>
        <Column Name="FECHA" DataType="System.DateTime"/>
        <Column Name="CAJERONOMBRE" DataType="System.String"/>
        <Column Name="CAJEROCLAVE" DataType="System.String"/>
        <Column Name="ID" DataType="System.Int64"/>
