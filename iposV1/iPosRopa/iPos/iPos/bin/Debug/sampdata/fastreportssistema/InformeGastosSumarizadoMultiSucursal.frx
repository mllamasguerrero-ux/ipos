namespace FastReport
{
  public class DatosReporte
  {
    private string claveGasto;
    public string ClaveGasto
    {
      get
      {
        return this.claveGasto;
      }
      set
      {
        this.claveGasto = value;
      }
    }
    
    private string nombreGasto;
    public string NombreGasto
    {
      get
      {
        return this.nombreGasto;
      }
      set
      {
        this.nombreGasto = value;
      }
    }
    
    private decimal total;
    public decimal Total
    {
      get
      {
        return this.total;
      }
      set
      {
        this.total = value;
      }
    }
    
    private string sucursal;
    public string Sucursal
    {
      get
      {
        return this.sucursal;
      }
      set
      {
        this.sucursal = value;
      }
    }
    
    
  }
  public class ReportScript
  {
    private void ImprimirRegistros(System.Collections.Generic.Dictionary&lt;string, DatosReporte&gt; datosP, List&lt;DatosReporte&gt; datos)
    {
      int contReg = 1;
      FastReport.Table.TableCell tc = null;
      FastReport.Table.TableRow tr =  new FastReport.Table.TableRow();
      Table2.PrintRow(0);
      
        if(((Boolean)Report.GetParameterValue(&quot;totalizado&quot;)))
        {
          foreach(KeyValuePair&lt;string, DatosReporte&gt; entry in datosP)
          {
            tr =  new FastReport.Table.TableRow();
            DatosReporte dr = entry.Value;
        
            tc = new FastReport.Table.TableCell();
            tc.Text = dr.ClaveGasto;
            tr.AddChild(tc);
        
            tc = new FastReport.Table.TableCell();
            tc.Text = dr.NombreGasto;
            tr.AddChild(tc);
        
            tc = new FastReport.Table.TableCell();
            tc.Text = dr.Total.ToString();
            tc.HorzAlign= FastReport.HorzAlign.Right;
            tc.Format = new FastReport.Format.CurrencyFormat();
            tr.AddChild(tc);
          
            tc = new FastReport.Table.TableCell();
            tc.Text = dr.Sucursal;
            tr.AddChild(tc);
       
            Table2.Rows.Add(tr);
            Table2.PrintRow(contReg); 
            Table2.PrintColumns();
            contReg++;
          }
        }
        else
        {
        foreach(DatosReporte dr in datos)
        {
          tr =  new FastReport.Table.TableRow();
        
          tc = new FastReport.Table.TableCell();
          tc.Text = dr.ClaveGasto;
          tr.AddChild(tc);
          
          tc = new FastReport.Table.TableCell();
          tc.Text = dr.NombreGasto;
          tr.AddChild(tc);
          
          tc = new FastReport.Table.TableCell();
          tc.Text = dr.Total.ToString();
          tc.HorzAlign= FastReport.HorzAlign.Right;
          tc.Format = new FastReport.Format.CurrencyFormat();
          tr.AddChild(tc);
          
          tc = new FastReport.Table.TableCell();
          tc.Text = dr.Sucursal;
          tr.AddChild(tc);
          
          Table2.Rows.Add(tr);
          Table2.PrintRow(contReg); 
          Table2.PrintColumns();
          contReg++;
        }
        }
    }  
    
    private void ImprimeFilas(ref int iCount, string strConexion, ref System.Collections.Generic.Dictionary&lt;string, DatosReporte&gt; datosP, ref List&lt;DatosReporte&gt; datos)
    {
      Report.Dictionary.Connections[0].ConnectionString = strConexion;
                                                 
      DataSourceBase rowDataSuc1 = Report.GetDataSource(&quot;Table&quot;); 
      rowDataSuc1.ForceLoadData = true;
      rowDataSuc1.Init();
      
      rowDataSuc1.First();
      
      DataSourceBase rowDataSuc2 = Report.GetDataSource(&quot;Suc&quot;); 
      rowDataSuc2.ForceLoadData = true;
      rowDataSuc2.Init();
      
      rowDataSuc2.First();
      
      if(((String)Report.GetParameterValue(&quot;succlave&quot;)) != &quot;&quot; &amp;&amp; ((String)Report.GetParameterValue(&quot;succlave&quot;)) != null)
      {
        
        if(((String)Report.GetParameterValue(&quot;succlave&quot;)).Equals(((String)Report.GetColumnValue(&quot;Suc.CLAVE&quot;))))
        {
          while(rowDataSuc1.HasMoreRows)
          {
            DatosReporte dr = new DatosReporte();
            dr.ClaveGasto = ((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;));
            dr.NombreGasto = ((String)Report.GetColumnValue(&quot;Table.NOMBREGASTO&quot;));
            dr.Total = ((Decimal)Report.GetColumnValue(&quot;Table.TOTAL&quot;));
            dr.Sucursal = ((String)Report.GetColumnValue(&quot;Suc.NOMBRE&quot;));
            datos.Add(dr); 
        
            rowDataSuc1.Next();
          }
        }
      }
      else
      {
        if(((Boolean)Report.GetParameterValue(&quot;totalizado&quot;)))
        {
          DatosReporte dr = new DatosReporte();
          while (rowDataSuc1.HasMoreRows /*&amp;&amp; rowDataSuc2.HasMoreRows*/)
          {                             
            dr = new DatosReporte();
            if(datosP.ContainsKey(((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;))))
            {
              dr = datosP[((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;))];
              try{
                dr.Total += ((Decimal)Report.GetColumnValue(&quot;Table.TOTAL&quot;));
              }
              catch(Exception ex)
              {
              
              } 
            }
            else
            {
              try{
                dr.ClaveGasto = ((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;)); 
              }
              catch(Exception ex)
              {
                dr.ClaveGasto = &quot;&quot;;
              }
              
              try{
                dr.NombreGasto = ((String)Report.GetColumnValue(&quot;Table.NOMBREGASTO&quot;));
              }
              catch(Exception ex)
              {
                dr.NombreGasto = &quot;&quot;;
              }
              
              try{
                dr.Total = ((Decimal)Report.GetColumnValue(&quot;Table.TOTAL&quot;));
              }
              catch(Exception ex)
              {
                dr.Total = 0;
              }
              
              try{
                dr.Sucursal = &quot;TODAS&quot;;
              }
              catch(Exception ex)
              {
                dr.Sucursal = &quot;&quot;;
              }
              
              
              datosP.Add(((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;)), dr);
            }
            rowDataSuc1.Next();
          }
        }
        else
        {
          while(rowDataSuc1.HasMoreRows)
          {
            DatosReporte dr = new DatosReporte();
            dr.ClaveGasto = ((String)Report.GetColumnValue(&quot;Table.CLAVEGASTO&quot;));
            dr.NombreGasto = ((String)Report.GetColumnValue(&quot;Table.NOMBREGASTO&quot;));
            dr.Total = ((Decimal)Report.GetColumnValue(&quot;Table.TOTAL&quot;));
            dr.Sucursal = ((String)Report.GetColumnValue(&quot;Suc.NOMBRE&quot;));
            datos.Add(dr); 
        
            rowDataSuc1.Next();
          }
        }
          
      }
    }

    private void Table2_ManualBuild(object sender, EventArgs e)
    {
      int iCount = 0; 
      string strCadenasConexion = ((String)Report.GetParameterValue(&quot;conexiones&quot;));//TextBox1.Text;//&quot;initial catalog=C:\\IposProject\\iPosRopa\\iPos\\iPos\\bin\\Debug\\sampdata\\IPOSDB.fdb;user id=sysdba;password=masterkey|initial catalog=C:\\IPOSTHINGS\\fama\\fama.fdb;user id=sysdba;password=masterkey&quot;;
      
      string[] cadenasConexion  = strCadenasConexion.Split(new string[] { &quot;masterkey&quot; }, StringSplitOptions.None); //strCadenasConexion.Split('|');
      
      System.Collections.Generic.Dictionary&lt;string, DatosReporte&gt; datosP = new System.Collections.Generic.Dictionary&lt;string, DatosReporte&gt;(); // string seria la clave de producto
      List&lt;DatosReporte&gt; datos = new List&lt;DatosReporte&gt;();
      
      foreach(string strCadena in cadenasConexion)
      {                                         
        if(strCadena.Trim().Length == 0)
          continue;
                        
        //MessageBox.Show(strCadena + &quot;masterkey&quot;);
       
        ImprimeFilas( ref iCount,  strCadena + &quot;masterkey&quot;, ref datosP, ref datos);
      }
      
      ImprimirRegistros(datosP, datos);
    }
  }
}
