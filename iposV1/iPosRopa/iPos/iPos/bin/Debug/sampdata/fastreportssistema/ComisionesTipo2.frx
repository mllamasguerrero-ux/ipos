      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;docto.fecha doctofecha,&#13;&#10;docto.folio doctofolio,&#13;&#10;doctopago.fecha pagofecha,&#13;&#10;coalesce(doctopago.fechaprocesado,doctopago.fecha) pagofechaprocesado,&#13;&#10;sucursal.clave clavesucursal,&#13;&#10;sucursal.nombre nombresucursal ,&#13;&#10;cajero.clave cajeroclave,&#13;&#10;cajero.nombre cajeronombre,&#13;&#10;persona.clave clienteclave,&#13;&#10;persona.nombre clientenombre,&#13;&#10;vendcliente.clave vendclienteclave,&#13;&#10;vendcliente.nombre vendclientenombre,&#13;&#10;&#13;&#10;(case when&#13;&#10; ((doctopago.fecha &gt;= @fechainicio and doctopago.fecha &lt;= @fechafin and doctopago.formapagoid in (1,2,5,15,20)) or&#13;&#10;  (coalesce(doctopago.fechaprocesado,current_date) &gt;= @fechainicio and coalesce(doctopago.fechaprocesado,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.aplicado,'N') = 'S')) then&#13;&#10;        doctopago.comision2&#13;&#10;  else&#13;&#10;        0&#13;&#10;   end&#13;&#10;&#13;&#10;   +&#13;&#10;case when&#13;&#10; ((coalesce(doctopago.fechadevuelto,current_date) &gt;= @fechainicio and coalesce(doctopago.fechadevuelto,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.devuelto,'N') = 'S')) then&#13;&#10;        doctopago.comision2  * - 1&#13;&#10;  else&#13;&#10;        0&#13;&#10;   end  ) comision2&#13;&#10;&#13;&#10;from&#13;&#10;doctopago&#13;&#10;inner join docto on docto.id = doctopago.doctoid&#13;&#10;left join parametro on 1 = 1&#13;&#10;left join sucursal on sucursal.id = parametro.sucursalid&#13;&#10;left join persona cajero on cajero.id = docto.vendedorid&#13;&#10;left join persona on persona.id = docto.personaid&#13;&#10;left join persona vendcliente on vendcliente.id = persona.vendedorid&#13;&#10;where&#13;&#10;((doctopago.fecha &gt;= @fechainicio and doctopago.fecha &lt;= @fechafin and doctopago.formapagoid in (1,2,5,15,20)) or&#13;&#10; (coalesce(doctopago.fechaprocesado,current_date) &gt;= @fechainicio and coalesce(doctopago.fechaprocesado,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.aplicado,'N') = 'S') or&#13;&#10; (coalesce(doctopago.fechadevuelto,current_date) &gt;= @fechainicio and coalesce(doctopago.fechadevuelto,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.devuelto,'N') = 'S'))&#13;&#10;and (docto.vendedorid = @usuarioid  or @usuarioid = 0 )&#13;&#10;and&#13;&#10;&#13;&#10;(&#13;&#10;    case when&#13;&#10;        ((doctopago.fecha &gt;= @fechainicio and doctopago.fecha &lt;= @fechafin and doctopago.formapagoid in (1,2,5,15,20)) or&#13;&#10;         (coalesce(doctopago.fechaprocesado,current_date) &gt;= @fechainicio and coalesce(doctopago.fechaprocesado,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.aplicado,'N') = 'S')) then&#13;&#10;            doctopago.comision2&#13;&#10;    else&#13;&#10;            0&#13;&#10;    end&#13;&#10;&#13;&#10;   +&#13;&#10;    case when&#13;&#10;        ((coalesce(doctopago.fechadevuelto,current_date) &gt;= @fechainicio and coalesce(doctopago.fechadevuelto,current_date) &lt;= @fechafin and doctopago.formapagoid in (3) and coalesce(doctopago.devuelto,'N') = 'S')) then&#13;&#10;            doctopago.comision2  * - 1&#13;&#10;    else&#13;&#10;            0&#13;&#10;    end&#13;&#10;)  &gt; 0&#13;&#10;">
        <Column Name="CLIENTECLAVE" DataType="System.String"/>
        <Column Name="CLIENTENOMBRE" DataType="System.String"/>
        <Column Name="CAJERONOMBRE" DataType="System.String"/>
        <Column Name="CAJEROCLAVE" DataType="System.String"/>
        <Column Name="CLAVESUCURSAL" DataType="System.String"/>
        <Column Name="NOMBRESUCURSAL" DataType="System.String"/>
