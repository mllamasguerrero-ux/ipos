    <FirebirdDataConnection Name="Connection" ConnectionString="rijcmlqDEsChHWgW5OFiwSyA3zls+AF0eyxuSCq7hxyjzZtEZP3nLeMVcUPo+VvPQYU0kjlSPeurNrOnJXwLPvTXv6YGynkdJxXoV5Jo//CE+sGNSQ=">
      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;docto.fecha doctofecha,&#13;&#10;SUM((case when docto.tipodoctoid = 22 then -1 else 1 end) * COALESCE(movto.total,0)) total,&#13;&#10;SUM((case when docto.tipodoctoid = 22 then -1 else 1 end) * COALESCE(movto.subtotal,0)) subtotal,&#13;&#10;SUM((case when docto.tipodoctoid = 22 then -1 else 1 end) * COALESCE(movto.iva,0)) iva,&#13;&#10;SUM((case when docto.tipodoctoid = 22 then -1 else 1 end) * COALESCE(movto.ieps,0)) ieps,&#13;&#10;sum( (case when movto.tipodoctoid = 22 then -1 else 1 end) *  (&#13;&#10; (( case when coalesce(estadorebaja,0) in (1,2) then coalesce(movto.precioconrebaja,0) else  coalesce(movto.precio,0) end )&#13;&#10; -  coalesce(movto.costoreposicion,0))  * coalesce(movto.cantidad,0) )  )  utilidad,&#13;&#10;sucursal.clave sucursalclave,&#13;&#10;sucursal.nombre sucursalnombre&#13;&#10;&#13;&#10; from movto&#13;&#10; left join docto on movto.doctoid = docto.id&#13;&#10;left join sucursal on sucursal.id = docto.sucursalid&#13;&#10; where  movto.cantidad &gt; 0 and docto.tipodoctoid in (21,22) and docto.fecha &gt;= @fechaini and docto.fecha &lt;= @fechafin and docto.estatusdoctoid = 1 and coalesce(docto.subtipodoctoid,0) not in (7,8)&#13;&#10;&#13;&#10;group by docto.fecha, sucursal.clave, sucursal.nombre">
        <Column Name="DOCTOFECHA" DataType="System.DateTime"/>
        <Column Name="SUBTOTAL" DataType="System.Decimal"/>
        <Column Name="IVA" DataType="System.Decimal"/>
        <Column Name="TOTAL" DataType="System.Decimal"/>
        <Column Name="UTILIDAD" DataType="System.Decimal"/>
        <Column Name="IEPS" DataType="System.Decimal"/>
        <Column Name="SUCURSALCLAVE" DataType="System.String"/>
        <Column Name="SUCURSALNOMBRE" DataType="System.String"/>
        <CommandParameter Name="fechaini" DataType="5" Expression="[fechaini]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="fechafin" DataType="5" Expression="[fechafin]" DefaultValue="01/01/2021"/>
      </TableDataSource>
      <TableDataSource Name="Table1" Alias="Empresa" DataType="System.Int32" Enabled="true" SelectCommand="select &#13;&#10;nombre,&#13;&#10;calle,&#13;&#10;numeroexterior,&#13;&#10;numerointerior,&#13;&#10;colonia,&#13;&#10;localidad,&#13;&#10;estado,&#13;&#10;cp,&#13;&#10;telefono,&#13;&#10;rfc,&#13;&#10;correoe&#13;&#10;&#13;&#10;from parametro;">
        <Column Name="NOMBRE" DataType="System.String"/>
        <Column Name="CALLE" DataType="System.String"/>
        <Column Name="NUMEROEXTERIOR" DataType="System.String"/>
        <Column Name="NUMEROINTERIOR" DataType="System.String"/>
        <Column Name="COLONIA" DataType="System.String"/>
        <Column Name="LOCALIDAD" DataType="System.String"/>
        <Column Name="ESTADO" DataType="System.String"/>
        <Column Name="CP" DataType="System.String"/>
        <Column Name="TELEFONO" DataType="System.String"/>
        <Column Name="RFC" DataType="System.String"/>
        <Column Name="CORREOE" DataType="System.String"/>
      </TableDataSource>
    </FirebirdDataConnection>    <Parameter Name="US_ID" DataType="System.Decimal"/>    <Parameter Name="US_NAME" DataType="System.String"/>
