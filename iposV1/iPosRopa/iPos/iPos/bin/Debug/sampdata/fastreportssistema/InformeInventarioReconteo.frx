      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select PRODUCTOID,PRODUCTOCLAVE,  SUM(CANTIDADSURTIDA) CANTIDADSURTIDA, sum(CAJAS) CAJAS, SUM(PIEZAS) PIEZAS, NOMBRE, DESCRIPCION1, ANAQUELID, ANAQUELCLAVE, ANAQUELNOMBRE, LOCALIDAD &#13;&#10;from (&#13;&#10;      SELECT PRODUCTO.ID PRODUCTOID, PRODUCTO.CLAVE PRODUCTOCLAVE,&#13;&#10;           SUM(COALESCE(CANTIDADSURTIDA,0)) CANTIDADSURTIDA,  &#13;&#10;           FLOOR( sum(COALESCE(CANTIDADSURTIDA,0))/(CASE WHEN COALESCE(PRODUCTO.pzacaja,0)= 0 THEN 1 else producto.pzacaja END)) CAJAS,&#13;&#10;           MOD( sum(COALESCE(CANTIDADSURTIDA,0)), (CASE WHEN COALESCE(PRODUCTO.pzacaja,0)= 0 THEN 1 else PRODUCTO.pzacaja END)) PIEZAS,&#13;&#10;           PRODUCTO.NOMBRE,&#13;&#10;           PRODUCTO.DESCRIPCION1 ,&#13;&#10;           MOVTO.ANAQUELID,&#13;&#10;           COALESCE(ANAQUEL.clave, 'Libre') ANAQUELCLAVE,&#13;&#10;           COALESCE(ANAQUEL.NOMBRE, 'Libre') ANAQUELNOMBRE,&#13;&#10;           MOVTO.LOCALIDAD&#13;&#10;       FROM PRODUCTO&#13;&#10;          INNER JOIN MOVTO ON PRODUCTO.ID = MOVTO.PRODUCTOID&#13;&#10;          LEFT JOIN ANAQUEL ON ANAQUEL.ID = MOVTO.ANAQUELID&#13;&#10;       WHERE (MOVTO.DOCTOID = @DOCTOID) AND&#13;&#10;    PRODUCTO.ID IN ( SELECT BUFFERVALUEID FROM BUFFERTEXT &#13;&#10;WHERE BUFFERGROUP = @BUFFERGROUP )&#13;&#10;&#13;&#10;          GROUP BY&#13;&#10;            PRODUCTO.ID,&#13;&#10;           PRODUCTO.CLAVE ,&#13;&#10;           PRODUCTO.NOMBRE,&#13;&#10;           PRODUCTO.DESCRIPCION1,&#13;&#10;           PRODUCTO.pzacaja,&#13;&#10;           MOVTO.ANAQUELID,     &#13;&#10;           ANAQUEL.clave ,&#13;&#10;           ANAQUEL.NOMBRE,&#13;&#10;           MOVTO.LOCALIDAD&#13;&#10;&#13;&#10;       union&#13;&#10;&#13;&#10;       SELECT PRODUCTO.ID PRODUCTOID,&#13;&#10;        PRODUCTO.CLAVE PRODUCTOCLAVE,&#13;&#10;           0 CANTIDADSURTIDA,&#13;&#10;           0 CAJAS,&#13;&#10;           0 PIEZAS,&#13;&#10;           PRODUCTO.NOMBRE,&#13;&#10;           PRODUCTO.DESCRIPCION1,&#13;&#10;           PRODUCTOLOCATIONS.ANAQUELID,&#13;&#10;           ANAQUEL.clave ANAQUELCLAVE,&#13;&#10;           ANAQUEL.NOMBRE ANAQUELNOMBRE,&#13;&#10;           PRODUCTOLOCATIONS.LOCALIDAD&#13;&#10;       FROM PRODUCTO&#13;&#10;       INNER JOIN  PRODUCTOLOCATIONS ON PRODUCTO.id = PRODUCTOLOCATIONS.PRODUCTOID &#13;&#10;          LEFT JOIN ANAQUEL ON ANAQUEL.ID = PRODUCTOLOCATIONS.ANAQUELID&#13;&#10;       WHERE &#13;&#10;    PRODUCTO.ID IN ( SELECT BUFFERVALUEID FROM BUFFERTEXT &#13;&#10;WHERE BUFFERGROUP = @BUFFERGROUP ) AND&#13;&#10;   PRODUCTOLOCATIONS.ALMACENID = (SELECT ALMACENID FROM DOCTO WHERE ID = @DOCTOID)&#13;&#10;&#13;&#10;               )&#13;&#10;group by PRODUCTOID,PRODUCTOCLAVE, NOMBRE, DESCRIPCION1, ANAQUELID, ANAQUELCLAVE, ANAQUELNOMBRE, LOCALIDAD&#13;&#10;       order by  PRODUCTOID">
