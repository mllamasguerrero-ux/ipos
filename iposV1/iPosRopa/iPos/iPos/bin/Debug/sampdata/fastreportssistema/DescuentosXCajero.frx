      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT        VENDEDOR.NOMBRE AS VENDEDORNOMBRE, VENDEDOR.USERNAME AS VENDEDORUSUARIO, DOCTO.FECHA, DOCTO.FOLIO, &#13;&#10;                         PRODUCTO.NOMBRE AS PRODUCTONOMBRE, PRODUCTO.DESCRIPCION AS PRODUCTODESCRIPCION, PRODUCTO.DESCRIPCION1 AS PRODUCTODESCRIPCION1,&#13;&#10;                          MOVTO.PRECIO, MOVTO.PRECIOLISTA, MOVTO.RAZONDESCUENTOCAJERO, MOVTO.CANTIDAD, MOVTO.PRECIOLISTA - MOVTO.PRECIO AS DIFERENCIAPRECIO, &#13;&#10;                         (MOVTO.PRECIOLISTA - MOVTO.PRECIO) * MOVTO.CANTIDAD AS DESCUENTOTOTAL&#13;&#10;FROM            MOVTO LEFT OUTER JOIN&#13;&#10;                         DOCTO ON DOCTO.ID = MOVTO.DOCTOID LEFT OUTER JOIN&#13;&#10;                         PRODUCTO ON PRODUCTO.ID = MOVTO.PRODUCTOID LEFT OUTER JOIN&#13;&#10;                         PERSONA VENDEDOR ON VENDEDOR.ID = DOCTO.VENDEDORID LEFT OUTER JOIN&#13;&#10;                         PERSONA CLIENTE ON CLIENTE.ID = DOCTO.PERSONAID&#13;&#10;WHERE        (DOCTO.ESTATUSDOCTOID = 1) AND (DOCTO.TIPODOCTOID = 21) AND (MOVTO.PRECIOLISTA &gt; MOVTO.PRECIO) AND (MOVTO.PRECIOMANUALMASBAJO = 'S') AND &#13;&#10;                         (DOCTO.FECHA &gt;= @FECHAINICIO) AND (DOCTO.FECHA &lt;= @FECHAFIN) AND (@VENDEDORID IN (DOCTO.VENDEDORID, 0))&#13;&#10;order by docto.vendedorid, docto.fecha, docto.id">
