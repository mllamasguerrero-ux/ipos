    <FirebirdDataConnection Name="Connection" ConnectionString="rijcmlqGXDUjVHoOo39U7VHz/cohboC87uIIffsgzROmdYrsoyLqNefFxFcJwrJj2UdQGssDygdH1v33WybpxKsRehsLaYYJhHd3d37jR6E5s5aXcYjs4SwwOJp6nOTF8aIB0KG7g2/iSAl7pKNV6hKceJCY+PrwGZoENC+eGjBMd0KWu4Gef0rNSOT/h1l/Je/4Opi">
      <TableDataSource Name="Table1" Alias="vendedor" DataType="System.Int32" Enabled="true" SelectCommand="select 0 as id, '' as clave, 'Todos # Vendedor: 0' as nombre from RDB$DATABASE&#13;&#10;union&#13;&#10;select id, clave, nombre ||  '  # Vendedor: ' || cast(id as varchar(13)) nombre from persona where tipopersonaid  in (20)&#13;&#10;">
        <Column Name="ID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;movto.productoid,&#13;&#10;producto.clave,&#13;&#10;producto.nombre,&#13;&#10;producto.descripcion,&#13;&#10;producto.descripcion1,&#13;&#10;producto.descripcion2, &#13;&#10;sucursal.clave clavesucursal,&#13;&#10;sucursal.nombre nombresucursal ,&#13;&#10;sum( case when docto.tipodoctoid = 21 then coalesce(movto.cantidad,0) else coalesce(movto.cantidad,0) * (-1) end) cantidad,&#13;&#10;&#13;&#10;&#13;&#10;case when sum( case when docto.tipodoctoid = 21 then coalesce(movto.total,0) else coalesce(movto.total,0) * (-1) end) = 0 then&#13;&#10;  0.0&#13;&#10;  else&#13;&#10;    sum( case when docto.tipodoctoid = 21 then coalesce(movto.cantidad,0) else coalesce(movto.cantidad,0) * (-1) end) /&#13;&#10;    sum( case when docto.tipodoctoid = 21 then coalesce(movto.total,0) else coalesce(movto.total,0) * (-1) end)&#13;&#10;  end   preciopromedio,&#13;&#10;&#13;&#10;sum( case when docto.tipodoctoid = 21 then coalesce(movto.total,0) else coalesce(movto.total,0) * (-1) end)  total,&#13;&#10;cajero.clave cajeroclave,&#13;&#10;cajero.nombre cajeronombre,&#13;&#10;persona.clave clienteclave,&#13;&#10;persona.nombre clientenombre,&#13;&#10;vendcliente.clave vendclienteclave,&#13;&#10;vendcliente.nombre vendclientenombre,&#13;&#10; coalesce(producto.comision,0.0) comision,&#13;&#10; sum( (case when docto.tipodoctoid = 21 then coalesce(movto.cantidad,0) else coalesce(movto.cantidad,0) * (-1) end) * coalesce(producto.comision,0.0)) totalcomision,&#13;&#10;docto.folio,&#13;&#10;movto.PRECIOCLASIFICACION,&#13;&#10;docto.fecha&#13;&#10;&#13;&#10;from&#13;&#10;docto left join movto  on movto.doctoid = docto.id&#13;&#10;left join docto doctoref on docto.doctorefid = doctoref.id&#13;&#10;left join producto on producto.id = movto.productoid&#13;&#10;left join parametro on 1 = 1&#13;&#10;left join sucursal on sucursal.id = parametro.sucursalid&#13;&#10;left join persona cajero on (&#13;&#10;    ( (docto.tipodoctoid = 21 or coalesce(doctoref.id,0) = 0) and cajero.id = docto.vendedorid)  or &#13;&#10;     (docto.tipodoctoid = 22 and  coalesce(doctoref.id,0) &lt;&gt;  0 and cajero.id = doctoref.vendedorid))&#13;&#10;left join persona on persona.id = docto.personaid&#13;&#10;left join persona vendcliente on vendcliente.id = persona.vendedorid&#13;&#10;where docto.fecha &gt;= @fechainicio and docto.fecha &lt;= @fechafin&#13;&#10;and docto.estatusdoctoid = 1&#13;&#10;and docto.tipodoctoid in (21,22)&#13;&#10;and (cajero.id = @usuarioid  or @usuarioid = 0 )&#13;&#10;&#13;&#10;group by&#13;&#10;movto.productoid,&#13;&#10;producto.clave,&#13;&#10;producto.nombre,&#13;&#10;producto.descripcion,&#13;&#10;producto.descripcion1,&#13;&#10;producto.descripcion2, &#13;&#10;sucursal.clave, &#13;&#10;sucursal.nombre ,&#13;&#10;cajero.clave ,&#13;&#10;cajero.nombre ,&#13;&#10;persona.clave ,&#13;&#10;persona.nombre ,&#13;&#10;vendcliente.clave ,&#13;&#10;vendcliente.nombre,&#13;&#10;producto.comision,&#13;&#10;docto.folio,&#13;&#10;movto.PRECIOCLASIFICACION,&#13;&#10;docto.fecha">
        <Column Name="CLIENTECLAVE" DataType="System.String"/>
        <Column Name="CLIENTENOMBRE" DataType="System.String"/>
        <Column Name="TOTAL" DataType="System.Decimal"/>
        <Column Name="CAJERONOMBRE" DataType="System.String"/>
        <Column Name="CAJEROCLAVE" DataType="System.String"/>
        <Column Name="PRODUCTOID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
        <Column Name="DESCRIPCION" DataType="System.String"/>
        <Column Name="DESCRIPCION1" DataType="System.String"/>
        <Column Name="DESCRIPCION2" DataType="System.String"/>
        <Column Name="CLAVESUCURSAL" DataType="System.String"/>
        <Column Name="NOMBRESUCURSAL" DataType="System.String"/>
        <Column Name="CANTIDAD" DataType="System.Decimal"/>
        <Column Name="PRECIOPROMEDIO" DataType="System.Decimal"/>
        <Column Name="VENDCLIENTECLAVE" DataType="System.String"/>
        <Column Name="VENDCLIENTENOMBRE" DataType="System.String"/>
        <Column Name="COMISION" DataType="System.Decimal"/>
        <Column Name="TOTALCOMISION" DataType="System.Decimal"/>
        <Column Name="FOLIO" DataType="System.Int32"/>
        <Column Name="PRECIOCLASIFICACION" DataType="System.Int32"/>
        <Column Name="FECHA" DataType="System.DateTime"/>
        <CommandParameter Name="fechainicio" DataType="5" Expression="[fechainicio]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="fechafin" DataType="5" Expression="[fechafin]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="usuarioid" DataType="1" Expression="vendedorId" DefaultValue="0"/>
      </TableDataSource>
    </FirebirdDataConnection>
