      </TableDataSource>
      <TableDataSource Name="Table1" DataType="System.Int32" Enabled="true" SelectCommand="select * from&#13;&#10;(&#13;&#10;select 0 ordenhelp, 0 as id ,current_date as fecha, 'Seleccione un inventario' as idfecha&#13;&#10;from parametro&#13;&#10;union all&#13;&#10;select 1 ordenhelp, id,fecha, (cast(id as varchar(10)) || '--' ||cast(fecha as varchar(15))) idfecha from docto where tipodoctoid = 50 and estatusdoctoid = 1&#13;&#10;)&#13;&#10;order by ordenhelp, fecha desc">
