      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select producto.clave, producto.nombre, producto.descripcion2, producto.descripcion1,&#13;&#10;movto.costoreposicion, movto.lote, movto.fechavence,&#13;&#10;coalesce(movto.cantidad,0) teorico,&#13;&#10;coalesce(movto.cantidadsurtida,0) fisico,&#13;&#10;coalesce(movto.cantidadsurtida,0) - coalesce(movto.cantidad,0) diferencia,&#13;&#10;(coalesce(movto.cantidadsurtida,0) - coalesce(movto.cantidad,0)) * (coalesce(movto.costoreposicion,0)) importediferencia&#13;&#10;  from docto&#13;&#10;inner join movto on docto.id = movto.doctoid&#13;&#10;inner join producto on producto.id = movto.productoid&#13;&#10;where docto.id = @doctoid&#13;&#10;and (@solodiferencias = 'N' or coalesce(movto.cantidadsurtida,0) - coalesce(movto.cantidad,0) &gt; 0)">
