    private void Text21_BeforePrint(object sender, EventArgs e)
    {
      int mes =  ((Int16)Report.GetColumnValue(&quot;Table.MES&quot;));
      switch(mes)
      {
        case 1 : Text21.Text = &quot;Enero&quot;; break;
        case 2 : Text21.Text = &quot;Febrero&quot;; break;
        case 3 : Text21.Text = &quot;Marzo&quot;; break;
        case 4 : Text21.Text = &quot;Abril&quot;; break;
        case 5 : Text21.Text = &quot;Mayo&quot;; break;
        case 6 : Text21.Text = &quot;Junio&quot;; break;
        case 7 : Text21.Text = &quot;Julio&quot;; break; 
        case 8 : Text21.Text = &quot;Agosto&quot;; break;
        case 9 : Text21.Text = &quot;Septiembre&quot;; break;
        case 10 : Text21.Text = &quot;Octubre&quot;; break;
        case 11 : Text21.Text = &quot;Noviembre&quot;; break;
        case 12 : Text21.Text = &quot;Diciembre&quot;; break;
        default : Text21.Text = &quot;&quot;; break;
      }
    }
  }
}
