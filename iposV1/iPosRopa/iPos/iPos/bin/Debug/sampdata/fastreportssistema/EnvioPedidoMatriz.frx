    private void Shape1_BeforePrint(object sender, EventArgs e)
    {                                     
          Decimal existencia = ((Decimal)Report.GetColumnValue(&quot;Table.EXISTENCIA&quot;)) ;
          Decimal aPedir = ((Decimal)Report.GetColumnValue(&quot;Table.CANTIDADSURTIDA&quot;));   
          String inventariable = ((String)Report.GetColumnValue(&quot;Table.INVENTARIABLE&quot;));
      
        if (aPedir &gt; existencia &amp;&amp; inventariable == &quot;S&quot;)
        {
          Shape1.Visible = true;
        }
        else
        {    
          Shape1.Visible = false;
        }
      
    }
  }
}
