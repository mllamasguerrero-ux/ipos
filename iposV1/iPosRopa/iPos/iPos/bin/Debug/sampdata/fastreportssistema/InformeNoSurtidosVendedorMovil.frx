  public class ReportScript
  {
    public long vendedorId = 0;               
    public long rutaId = 0;

   

    private void CBVendedor_TextChanged(object sender, EventArgs e)
    {
      string[] split = CBVendedor.Text.Split(new string[] { &quot;# Vendedor: &quot; }, StringSplitOptions.None);
      string strNumCliente = &quot;0&quot;;
      if(split.Length &gt; 1)     
        strNumCliente = split[1];
      
      try
      {
        vendedorId = long.Parse(strNumCliente);
      }
      catch{
      }
    }
        
  

    private void Form1_Load(object sender, EventArgs e)
    {    
      
      DateTimePicker1.Value = DateTime.Today;  
      DateTimePicker2.Value = DateTime.Today;
    }
                     

    

    private void CBRuta_TextChanged(object sender, EventArgs e)
    {
          
      string[] split = CBRuta.Text.Split(new string[] { &quot;# Ruta: &quot; }, StringSplitOptions.None);
      string strItem = &quot;0&quot;;
      if(split.Length &gt; 1)     
        strItem = split[1];
      
      try
      {
        rutaId = long.Parse(strItem);
      }
      catch{
      }
      
      
    }
  }
}
</ScriptText>
