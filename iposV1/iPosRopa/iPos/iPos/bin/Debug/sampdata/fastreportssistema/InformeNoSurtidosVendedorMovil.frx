        <Column Name="PRODUCTODESCRIPCION1" DataType="System.String"/>
        <Column Name="CANTIDADPEDIDA" DataType="System.Decimal"/>
        <Column Name="CANTIDADSURTIDA" DataType="System.Decimal"/>
        <Column Name="CANTIDADFALTANTE" DataType="System.Decimal"/>
        <Column Name="PRECIOUNITARIO" DataType="System.Decimal"/>
        <Column Name="IMPORTEFALTANTE" DataType="System.Decimal"/>
        <CommandParameter Name="fechainicial" DataType="5" Expression="[fechainicio]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="fechafinal" DataType="5" Expression="[fechafin]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="vendedorfinalid" DataType="1" Expression="vendedorId" DefaultValue="0"/>
        <CommandParameter Name="rutafinalid" DataType="1" Expression="rutaId" DefaultValue="0"/>
      </TableDataSource>
      <TableDataSource Name="Table" Alias="ruta" DataType="System.Int32" Enabled="true" SelectCommand="select 0 as id, '' as clave, 'Todos # Ruta: 0' as nombre from RDB$DATABASE&#13;&#10;union&#13;&#10;select id, clave, nombre ||  '  # Ruta: ' || cast(id as varchar(13)) nombre from rutaembarque&#13;&#10;">
        <Column Name="ID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
      </TableDataSource>
