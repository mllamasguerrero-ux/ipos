      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;cliente.clave clienteclave,&#13;&#10;cliente.nombre clientenombre,&#13;&#10;cliente.nombres clientenombres,&#13;&#10;cliente.apellidos  clienteapellidos,&#13;&#10;vendedor.clave vendedorclave,&#13;&#10;vendedor.nombre vendedornombre,&#13;&#10;doctoventa.fecha doctofecha,&#13;&#10;doctoventa.folio doctofolio,&#13;&#10;producto.clave productoclave,&#13;&#10;producto.descripcion productodescripcion,&#13;&#10;producto.descripcion1 productodescripcion1,&#13;&#10;movto.cantidad cantidadpedida,&#13;&#10;movto.cantidadsurtida cantidadsurtida,&#13;&#10;movto.cantidadfaltante cantidadfaltante,&#13;&#10;round((movto.precio  * cast( (1.0000 + ( (CASE WHEN COALESCE(PM.desgloseieps,'N') = 'S' THEN movto.tasaieps ELSE 0.0000 END) /100.0000)) * (1.0000 + (movto.tasaiva/100.0000)) as numeric(18,4))),2) preciounitario,&#13;&#10;round((movto.precio  * cast( (1.0000 + ( (CASE WHEN COALESCE(PM.desgloseieps,'N') = 'S' THEN movto.tasaieps ELSE 0.0000 END) /100.0000)) * (1.0000 + (movto.tasaiva/100.0000)) as numeric(18,4))),2) *  movto.cantidadfaltante  importefaltante&#13;&#10;&#13;&#10;from docto&#13;&#10;left join movto on movto.doctoid = docto.id&#13;&#10;left join docto doctoventa on doctoventa.id = docto.doctorefid&#13;&#10;left join producto on producto.id = movto.productoid&#13;&#10;left join parametro pm on 1 = 1&#13;&#10;left join persona cliente on cliente.id = docto.personaid&#13;&#10;left join persona vendedor on vendedor.id = docto.vendedorfinal&#13;&#10;&#13;&#10;where docto.tipodoctoid = 331 and docto.estatusdoctoid = 1 and coalesce(docto.doctorefid,0) &lt;&gt; 0&#13;&#10;and docto.fecha &gt;= @fechainicial and docto.fecha &lt;= @fechafinal&#13;&#10;and (coalesce(docto.vendedorfinal,0) = @vendedorfinalid or @vendedorfinalid = 0)&#13;&#10;and (coalesce(doctoventa.rutaembarqueid,0) = @rutafinalid or @rutafinalid = 0)&#13;&#10;and coalesce(movto.cantidadfaltante,0) &gt; 0&#13;&#10;order by docto.fecha&#13;&#10;">
        <Column Name="CLIENTECLAVE" DataType="System.String"/>
        <Column Name="CLIENTENOMBRE" DataType="System.String"/>
        <Column Name="CLIENTENOMBRES" DataType="System.String"/>
        <Column Name="CLIENTEAPELLIDOS" DataType="System.String"/>
        <Column Name="VENDEDORCLAVE" DataType="System.String"/>
        <Column Name="VENDEDORNOMBRE" DataType="System.String"/>
        <Column Name="DOCTOFECHA" DataType="System.DateTime"/>
        <Column Name="DOCTOFOLIO" DataType="System.Int32"/>
        <Column Name="PRODUCTOCLAVE" DataType="System.String"/>
