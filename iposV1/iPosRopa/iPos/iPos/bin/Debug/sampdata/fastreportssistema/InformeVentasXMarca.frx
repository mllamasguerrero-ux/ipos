      Text22.Text += productoDescripcion1 + productoDescripcion2 + texto1 + texto2 + texto3 + texto4 + texto5 + texto6;
      
      
    }

    private void Text1_BeforePrint(object sender, EventArgs e)
    {
      
      if(  ((Decimal)Report.GetParameterValue(&quot;itemid&quot;)) == 0)
      {
        Text1.Text = &quot;Todos&quot;;
      }
      else
      {
        Text1.Text = ((String)Report.GetColumnValue(&quot;Marca.CLAVE&quot;)) + &quot; &quot; +  ((String)Report.GetColumnValue(&quot;Marca.NOMBRE&quot;));
      }  
    }
