        <Column Name="ID" DataType="System.Int64"/>
        <Column Name="CLAVE" DataType="System.String"/>
        <Column Name="NOMBRE" DataType="System.String"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="Main" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;   I.FECHAHORA ,&#13;&#10;   TD.NOMBRE TIPODOCTONOMBRE,&#13;&#10;   V.NOMBRE USUARIONOMBRE,&#13;&#10;   P.CLAVE PRODUCTOCLAVE,&#13;&#10;   P.NOMBRE PRODUCTONOMBRE,&#13;&#10;   T.CLAVE TIPOINCIDENCIACLAVE,&#13;&#10;   T.NOMBRE TIPOINCIDENCIANOMBRE,&#13;&#10;   D.FOLIO FOLIODOCTO,&#13;&#10;   I.NOTA1,&#13;&#10;   I.NOTA2,&#13;&#10;   I.CANTIDAD1,&#13;&#10;   I.CANTIDAD2&#13;&#10;FROM INCIDENCIA I&#13;&#10;LEFT JOIN TIPOINCIDENCIA T ON I.TIPOINCIDENCIAID = T.ID&#13;&#10;LEFT JOIN PERSONA V ON V.ID = I.VENDEDORID&#13;&#10;LEFT JOIN PRODUCTO P ON P.ID = I.PRODUCTOID&#13;&#10;LEFT JOIN DOCTO D ON D.ID = I.DOCTOID&#13;&#10;LEFT JOIN TIPODOCTO TD ON TD.ID = D.TIPODOCTOID&#13;&#10;WHERE CAST(I.FECHAHORA AS DATE) &gt;= @fechainicio AND CAST(I.FECHAHORA AS DATE) &lt;=@fechafin&#13;&#10;AND (@usuarioid = 0 or @usuarioid = coalesce(i.vendedorid,0))&#13;&#10;order by i.fechahora">
        <Column Name="TIPODOCTONOMBRE" DataType="System.String"/>
        <Column Name="FECHAHORA" DataType="System.DateTime"/>
        <Column Name="USUARIONOMBRE" DataType="System.String"/>
        <Column Name="PRODUCTOCLAVE" DataType="System.String"/>
        <Column Name="PRODUCTONOMBRE" DataType="System.String"/>
        <Column Name="TIPOINCIDENCIACLAVE" DataType="System.String"/>
        <Column Name="TIPOINCIDENCIANOMBRE" DataType="System.String"/>
        <Column Name="FOLIODOCTO" DataType="System.Int32"/>
        <Column Name="NOTA1" DataType="System.String"/>
        <Column Name="NOTA2" DataType="System.String"/>
        <Column Name="CANTIDAD1" DataType="System.Decimal"/>
        <Column Name="CANTIDAD2" DataType="System.Decimal"/>
        <CommandParameter Name="fechainicio" DataType="5" Expression="[fechainicio]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="fechafin" DataType="5" Expression="[fechafin]" DefaultValue="01/01/2001"/>
        <CommandParameter Name="usuarioid" DataType="1" Expression="vendedorId" DefaultValue="0"/>
      </TableDataSource>
    </FirebirdDataConnection>    <Parameter Name="US_ID" DataType="System.Decimal"/>    <Parameter Name="US_NAME" DataType="System.String"/>
    <Parameter Name="vendedorid" DataType="System.Decimal"/>
    <Parameter Name="fechainicio" DataType="System.DateTime"/>
    <Parameter Name="fechafin" DataType="System.DateTime"/>
  </Dictionary>
