    private void CBFacturaRemision_SelectedIndexChanged(object sender, EventArgs e)
    {
      Report.SetParameterValue(&quot;REMISIONFACTURADESC&quot;, CBFacturaRemision.Text);
      
      switch(CBFacturaRemision.SelectedIndex)
      {
        case 0:   Report.SetParameterValue(&quot;REMISIONFACTURA&quot;, &quot;T&quot;); break; 
        case 1:   Report.SetParameterValue(&quot;REMISIONFACTURA&quot;, &quot;F&quot;); break;
        case 2:   Report.SetParameterValue(&quot;REMISIONFACTURA&quot;, &quot;R&quot;); break;
        default: Report.SetParameterValue(&quot;REMISIONFACTURA&quot;, &quot;T&quot;); break;
       
      }
    }

    private void _StartReport(object sender, EventArgs e)
    {
      Report.SetParameterValue(&quot;REMISIONFACTURA&quot;, &quot;T&quot;); 
      Report.SetParameterValue(&quot;MOSTRARVENTA&quot;, &quot;S&quot;);   
      Report.SetParameterValue(&quot;MOSTRARTRASPASO&quot;, &quot;S&quot;);  
      Report.SetParameterValue(&quot;MOSTRARNC&quot;, &quot;S&quot;);          
