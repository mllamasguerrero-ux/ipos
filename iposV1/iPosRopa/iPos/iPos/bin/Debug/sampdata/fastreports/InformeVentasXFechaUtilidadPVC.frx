      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;&#13;&#10;producto.id productoid,&#13;&#10;producto.clave productoclave,&#13;&#10;producto.nombre productonombre,&#13;&#10;producto.descripcion productodescripcion ,&#13;&#10;producto.descripcion1 productodescripcion1,&#13;&#10;producto.descripcion2 productodescripcion2,&#13;&#10;producto.descripcion3 productodescripcion3,&#13;&#10;&#13;&#10;&#13;&#10;sum(coalesce(movto.cantidad,0) * coalesce(td.sentidoinventario,0) * (-1)) cantidad,&#13;&#10;sum(coalesce(movto.importe,0) * coalesce(td.sentidopago,0)) importe,&#13;&#10;sum(coalesce(movto.subtotal,0) * coalesce(td.sentidopago,0)) subtotal,&#13;&#10;sum(coalesce(movto.iva,0) * coalesce(td.sentidopago,0)) iva,&#13;&#10;sum(coalesce(movto.descuento,0) * coalesce(td.sentidopago,0)) descuento,&#13;&#10;sum(coalesce(movto.total,0) * coalesce(td.sentidopago,0)) total  ,&#13;&#10;sum(coalesce(movto.utilidad,0) * coalesce(td.sentidopago,0)) utilidad  ,&#13;&#10;avg(coalesce(movto.precio,0)) preciopromedio,&#13;&#10;avg(coalesce(movto.costoreposicion,0)) costoreposicionpromedio ,&#13;&#10;sum( case when docto.tipodoctoid = 21 then 1 else  0 end ) numerodeventas,&#13;&#10;sum( case when docto.tipodoctoid = 22 then 1 else  0 end ) numerodenc&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10; from movto&#13;&#10; left join docto on movto.doctoid = docto.id&#13;&#10; left join producto on producto.id = movto.productoid&#13;&#10;left join tipodocto td on td.id = docto.tipodoctoid&#13;&#10; where  movto.cantidad &gt; 0 and docto.tipodoctoid in (21,22) and  &#13;&#10; docto.fecha &gt;= @fechaini and docto.fecha &lt;= @fechafin&#13;&#10; and docto.estatusdoctoid = 1&#13;&#10;&#13;&#10;group by&#13;&#10;&#13;&#10;producto.id ,&#13;&#10;producto.clave ,&#13;&#10;producto.nombre ,&#13;&#10;producto.descripcion  ,&#13;&#10;producto.descripcion1 ,&#13;&#10;producto.descripcion2 ,&#13;&#10;producto.descripcion3&#13;&#10;&#13;&#10;order by producto.clave">
