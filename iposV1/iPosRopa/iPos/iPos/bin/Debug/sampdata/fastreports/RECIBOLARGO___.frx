    private void Text17_BeforePrint(object sender, EventArgs e)
    {
      if(((String)Report.GetColumnValue(&quot;REC_MASTER.NOTAPAGO&quot;)).Trim().Length &gt; 0)
      {
        Text17.Text = &quot;Nota: &quot; + ((String)Report.GetColumnValue(&quot;REC_MASTER.NOTAPAGO&quot;));
      }
    }

    private void Text18_BeforePrint(object sender, EventArgs e)
    {
      if(  Report.GetColumnValue(&quot;REC_MASTER.FECHAFACTURA&quot;) == null)
        return;
      
      if( ((DateTime)Report.GetColumnValue(&quot;REC_MASTER.FECHAFACTURA&quot;)) &lt; DateTime.Parse(&quot;01/01/2000&quot;))
        return;
                                                                        
      Text18.Text = ((String)Report.GetColumnValue(&quot;REC_MASTER.DOC_REF_HEADER5&quot;)) + &quot;  Fecha Factura: &quot; +  ((DateTime)Report.GetColumnValue(&quot;REC_MASTER.FECHAFACTURA&quot;)).ToString(&quot;dd/MM/yyyy&quot;);
    }

    private void Picture1_BeforePrint(object sender, EventArgs e)
    {           
           Picture1.ImageLocation =  ((String)Report.GetColumnValue(&quot;REC_MASTER.LOGOIMAGE&quot;)).Replace(&quot;{BaseDirectory}&quot;, System.IO.Path.GetDirectoryName(Report.FileName ) + &quot;\\..\\..&quot; ) ;    
    }

    private void Text22_BeforePrint(object sender, EventArgs e)
    {                   
      
      
      if(((Decimal)Report.GetColumnValue(&quot;REC_MASTER.SALDO&quot;)) &gt; 0) 
      {
        Text22.Visible = true;
        Text22.Text = Text22.Text.Replace(&quot;(CANTIDAD CON LETRA)&quot;,NumaletRpt.ToCardinal(((Decimal)Report.GetColumnValue(&quot;REC_MASTER.TOTAL&quot;))).ToUpper());
      }
      else   
        Text22.Visible = false;

        
    }
  }
