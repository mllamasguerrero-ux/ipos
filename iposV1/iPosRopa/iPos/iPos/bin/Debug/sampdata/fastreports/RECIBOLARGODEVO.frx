      <TableDataSource Name="Table1" Alias="REC_DETAIL" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;        MOVTO.doctoid,&#13;&#10;&#13;&#10;        (CASE&#13;&#10;         WHEN (docto.tipodoctoid in  (41)) THEN MOVTO.cantidadfaltante&#13;&#10;         ELSE MOVTO.cantidad&#13;&#10;        END ) cantidad,&#13;&#10;&#13;&#10;       &#13;&#10;        CASE WHEN PRODUCTO.descripcion_comodin = 'S' AND MOVTO.descripcion1 IS NOT NULL AND MOVTO.descripcion1 &lt;&gt; '' THEN MOVTO.DESCRIPCION1 ELSE  PRODUCTO.DESCRIPCION1 END&#13;&#10;&#13;&#10;        AS DESCRIPCION1,&#13;&#10;&#13;&#10;        CASE WHEN PRODUCTO.descripcion_comodin = 'S' AND MOVTO.descripcion1 IS NOT NULL AND MOVTO.descripcion1 &lt;&gt; '' THEN MOVTO.DESCRIPCION2 ELSE  PRODUCTO.DESCRIPCION2 END AS DESCRIPCION2,&#13;&#10;&#13;&#10;        (CASE&#13;&#10;         WHEN (docto.tipodoctoid in  (11,41)) THEN MOVTO.costo&#13;&#10;         ELSE MOVTO.precio&#13;&#10;        END ) PRECIO,&#13;&#10;&#13;&#10;        MOVTO.descuento,&#13;&#10;&#13;&#10;        (CASE&#13;&#10;         WHEN (docto.tipodoctoid in  (41)) THEN round(MOVTO.subtotal * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2)&#13;&#10;         ELSE MOVTO.subtotal&#13;&#10;         END ) subtotal,&#13;&#10;&#13;&#10;        PRODUCTO.tasaiva TASAIVA,&#13;&#10;&#13;&#10;        CASE WHEN docto.tipodoctoid = 21 then MOVTO.total + coalesce(MOVTO.DESCUENTOVALE,0) &#13;&#10;                 when docto.tipodoctoid = 41 then  round(MOVTO.total * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2)&#13;&#10;                 else MOVTO.total end total,&#13;&#10;&#13;&#10;&#13;&#10;        MOVTO.lote,&#13;&#10;        MOVTO.fechavence ,&#13;&#10;        movto.cantidadfaltante ,&#13;&#10;        MOVTO.cantidadsurtida  ,&#13;&#10;        MOVTO.cantidad ,&#13;&#10;        round(MOVTO.importedescuento * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2),&#13;&#10;        round(MOVTO.iva * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2) IMPORTEIVA ,&#13;&#10;        coalesce(MOVTO.DESCUENTOVALE,0) descuentovale,&#13;&#10;        linea.tiporecarga ,&#13;&#10;&#13;&#10;&#13;&#10;        (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                ELSE&#13;&#10;                    cast(cast(COALESCE(MOVTO.precio,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                END ) PRECIOCONIVA  ,&#13;&#10;&#13;&#10;                c.texto1,&#13;&#10;                c.texto2,&#13;&#10;                c.texto3,&#13;&#10;                c.texto4,&#13;&#10;                c.texto5,&#13;&#10;                c.texto6,&#13;&#10;                c.numero1,&#13;&#10;                c.numero2,&#13;&#10;                c.numero3,&#13;&#10;                c.numero4,&#13;&#10;                c.fecha1, &#13;&#10;                c.fecha2,&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;           &#13;&#10;        CASE WHEN PRODUCTO.descripcion_comodin = 'S' AND MOVTO.descripcion1 IS NOT NULL AND MOVTO.descripcion1 &lt;&gt; '' THEN MOVTO.DESCRIPCION3 ELSE  PRODUCTO.DESCRIPCION3 END AS DESCRIPCION3 ,&#13;&#10;        PRODUCTO.descripcion_comodin ,&#13;&#10;        PRODUCTO.manejalote  ,&#13;&#10;                             &#13;&#10;        MOVTO.TASAIEPS TASAIEPS ,&#13;&#10;       round(MOVTO.IEPS * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2) IMPORTEIEPS ,&#13;&#10;        (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;&#13;&#10;                        COALESCE(MOVTO.costo,0) * ((100 + coalesce(MOVTO.tasaieps,0)) / 100 )&#13;&#10;                ELSE&#13;&#10;                    COALESCE(MOVTO.precio,0) * ((100 + coalesce(MOVTO.tasaieps,0)) / 100 )&#13;&#10;                END ) PRECIOCONIEPS  ,&#13;&#10;                               &#13;&#10;        MOVTO.TASAIMPUESTO TASAIMPUESTO ,&#13;&#10;        round(MOVTO.IMPUESTO * coalesce(movto.cantidadfaltante,0)/coalesce(movto.cantidad,0),2) IMPORTEIMPUESTO ,&#13;&#10;&#13;&#10;        (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                ELSE&#13;&#10;                    cast(cast(COALESCE(MOVTO.precio,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                END ) PRECIOCONIMPUESTO,&#13;&#10;&#13;&#10;&#13;&#10;case when docto.subtipodoctoid = 13 then '[DPP]' || producto.clave else producto.clave end productoclave    ,&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;movto.tasaieps,&#13;&#10;&#13;&#10;movto.descuentoporcentaje,&#13;&#10;&#13;&#10;producto.pzacaja,&#13;&#10;&#13;&#10;case&#13;&#10;    when  producto.unidad = 'KG' then&#13;&#10;         cast(     (CASE WHEN (docto.tipodoctoid in  (41)) THEN cast(MOVTO.cantidadfaltante as numeric(18,3)) ELSE cast(MOVTO.cantidad as numeric(18,3)) END ) as varchar(20) ) || (case when (CASE WHEN (docto.tipodoctoid in  (41)) THEN cast(MOVTO.cantidadfaltante as numeric(18,3)) ELSE cast(MOVTO.cantidad as numeric(18,3)) END )  &gt;= 1 then ' KG' else ' GM' end)&#13;&#10;    when producto.unidad &lt;&gt; 'PZA' and producto.unidad &lt;&gt; 'KG' then&#13;&#10;        cast(     (CASE WHEN (docto.tipodoctoid in  (41)) THEN MOVTO.cantidadfaltante ELSE MOVTO.cantidad END )  as varchar(20))&#13;&#10;    when /*producto.unidad = 'PZA' and*/ ( coalesce(producto.pzacaja,0) = 0 or  coalesce(producto.pzacaja,0) = 1 ) then&#13;&#10;         cast(     (CASE WHEN (docto.tipodoctoid in  (41)) THEN MOVTO.cantidadfaltante ELSE MOVTO.cantidad END )  as varchar(20)) || ' PZAS'&#13;&#10;    else&#13;&#10;        cast(trunc(coalesce(     (CASE WHEN (docto.tipodoctoid in  (41)) THEN MOVTO.cantidadfaltante ELSE MOVTO.cantidad END   )   ,0)/coalesce(producto.pzacaja,1)) as varchar(20)) || ' cajas ' ||  cast(mod(coalesce(    (CASE WHEN (docto.tipodoctoid in  (41)) THEN MOVTO.cantidadfaltante ELSE MOVTO.cantidad END )   ,0),coalesce(producto.pzacaja,1)) as varchar(10)) || ' piezas '&#13;&#10;end &#13;&#10;&#13;&#10;descpzacaja,&#13;&#10;&#13;&#10;case when COALESCE(movto.descuentoporcentaje,0) &lt; 100 then&#13;&#10;round(&#13;&#10;      (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                ELSE&#13;&#10;                    cast(cast(COALESCE(MOVTO.preciolista,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                END ) &#13;&#10;, 2)&#13;&#10;  else&#13;&#10;     0.00&#13;&#10;  end&#13;&#10;&#13;&#10;PRECIOCONIMPUESTOSINDESCUENTO,&#13;&#10;&#13;&#10;&#13;&#10;(&#13;&#10;&#13;&#10;case when COALESCE(movto.descuentoporcentaje,0) &lt; 100 then&#13;&#10;round(&#13;&#10;      (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                ELSE&#13;&#10;                    cast(cast(COALESCE(MOVTO.preciolista,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                END ) &#13;&#10;, 2)&#13;&#10;  else&#13;&#10;     0.00&#13;&#10;  end&#13;&#10;&#13;&#10;  -&#13;&#10;&#13;&#10; (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                ELSE&#13;&#10;                    cast(cast(COALESCE(MOVTO.precio,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.tasaiva,0)) / 100 ) as d_importe)&#13;&#10;                END )&#13;&#10;&#13;&#10;&#13;&#10;) &#13;&#10; DESCUENTOUNITARIO,&#13;&#10;&#13;&#10;movto.preciomaximopublico ,&#13;&#10;&#13;&#10;&#13;&#10;coalesce(MKITREF.id,0) KITMOVTOID,&#13;&#10;PKITREF.CLAVE  KITPRODCLAVE,&#13;&#10;PKITREF.nombre KITPRODNOMBRE,&#13;&#10;PKITREF.descripcion1 KITDESCRIPCION1,&#13;&#10;MKITREF.preciolista KITPRECIOLISTA,&#13;&#10;MKITREF.precio KITPRECIO,&#13;&#10;MKITREF.tasaiva KITTASAIVA,&#13;&#10;MKITREF.tasaieps KITTASAIEPS,&#13;&#10;MKITREF.tasaimpuesto KITIMPUESTO,&#13;&#10;MKITREF.iva KITIVA,&#13;&#10;MKITREF.ieps KITIEPS,&#13;&#10;MKITREF.subtotal KITSUBTOTAL,&#13;&#10;MKITREF.TOTAL KITTOTAL,&#13;&#10;MKITREF.cantidad KITCANTIDAD,&#13;&#10;&#13;&#10;case when coalesce(PKITREF.pzacaja,0) = 0 then&#13;&#10;cast(MKITREF.cantidad as varchar(10)) || ' pzas' else&#13;&#10;cast(trunc(coalesce(MKITREF.cantidad,0)/coalesce(PKITREF.pzacaja,1)) as varchar(10)) || ' cajas ' ||  cast(mod(coalesce(MKITREF.cantidad,0),coalesce(PKITREF.pzacaja,1)) as varchar(10)) || ' piezas '&#13;&#10;end  KITDESCPZACAJA  ,&#13;&#10;&#13;&#10;MOVTO.ID MOVTOID,&#13;&#10;coalesce(PRODUCTO.ESKIT,'N') ESKIT,&#13;&#10;&#13;&#10;&#13;&#10; (CASE   WHEN (docto.tipodoctoid in  (11,41)) THEN&#13;&#10;                   cast(cast(COALESCE(MOVTO.costo,0)  * ((100 + cast(coalesce(MOVTO.tasaieps,0) as numeric(10,4))) / 100 ) as d_importe)  * ((100 + coalesce(MOVTO.t