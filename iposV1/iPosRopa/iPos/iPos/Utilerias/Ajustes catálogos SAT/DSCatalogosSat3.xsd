                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select sat_productoservicio.id,
       sat_productoservicio.sat_claveprodserv,
       sat_productoservicio.sat_descripcion,
       sat_productoservicio.sat_fechainicio,
       sat_productoservicio.sat_fechafin,
       sat_productoservicio.sat_ivatrasladado,
       sat_productoservicio.sat_iepstrasladado,
       sat_productoservicio.sat_complemento
 from sat_productoservicio where @tipo = 'Todos'
union
select sat_productoservicio.id,
       sat_productoservicio.sat_claveprodserv,
       sat_productoservicio.sat_descripcion,
       sat_productoservicio.sat_fechainicio,
       sat_productoservicio.sat_fechafin,
       sat_productoservicio.sat_ivatrasladado,
       sat_productoservicio.sat_iepstrasladado,
       sat_productoservicio.sat_complemento
from sat_productoservicio_oper inner join sat_productoservicio on sat_productoservicio_oper.sat_productoservicioid = sat_productoservicio.id
where @tipo = 'Parcial' 
union
select sat_productoservicio.id,
       sat_productoservicio.sat_claveprodserv,
       sat_productoservicio.sat_descripcion,
       sat_productoservicio.sat_fechainicio,
       sat_productoservicio.sat_fechafin,
       sat_productoservicio.sat_ivatrasladado,
       sat_productoservicio.sat_iepstrasladado,
       sat_productoservicio.sat_complemento from sat_productoservicio_linea inner join sat_productoservicio on sat_productoservicio_linea.sat_productoservicioid = sat_productoservicio.id
where @tipo = 'Linea' and sat_productoservicio_linea.lineaid = @lineaid</CommandText>
