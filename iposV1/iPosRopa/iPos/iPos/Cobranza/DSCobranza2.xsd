                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select
contrarecibohdr.id,
contrarecibohdr.activo,
contrarecibohdr.fecha,
contrarecibohdr.personaid,
contrarecibohdr.vendedorid,
contrarecibohdr.estatusid,
contrarecibohdr.total,
contrarecibohdr.abono,
contrarecibohdr.saldo,
contrarecibohdr.folio,
contrarecibohdr.observaciones ,
persona.nombre nombrecliente,
estatusdocto.nombre nombreestatus,
estadopagocontrarecibo.nombre nombreestatuspago
 from
contrarecibohdr left join contrarecibodtl
    on contrarecibohdr.id = contrarecibodtl.contrareciboid
    left join persona on persona.id = contrarecibohdr.personaid
    left join estatusdocto on estatusdocto.id = contrarecibohdr.estatusid
    left join estadopagocontrarecibo on  estadopagocontrarecibo.id = contrarecibohdr.estatuspagoid
 where 
((@folio in (0, contrarecibohdr.folio))) and
(( @filtrarfecha &lt;&gt; 'S' or contrarecibohdr.fecha &gt;= @fechaini and contrarecibohdr.fecha &lt;=@fechafin)) and
((@personaid in (0, contrarecibohdr.personaid))) and
((@venta in (0, contrarecibodtl.folio))) and

((@estatusid = -1 and contrarecibohdr.estatusid &lt;&gt; 2) or contrarecibohdr.estatusid = @estatusid)
group by
contrarecibohdr.id,
contrarecibohdr.activo,
contrarecibohdr.fecha,
contrarecibohdr.personaid,
contrarecibohdr.vendedorid,
contrarecibohdr.estatusid,
contrarecibohdr.total,
contrarecibohdr.abono,
contrarecibohdr.saldo,
contrarecibohdr.folio,
contrarecibohdr.observaciones  ,
persona.nombre,
estatusdocto.nombre ,
estadopagocontrarecibo.nombre</CommandText>
