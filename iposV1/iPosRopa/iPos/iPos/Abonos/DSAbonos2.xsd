                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
  SUCURSAL.ID SUCURSALID,
  SUCURSAL.CLAVE SUCURSALCLAVE,
  SUCURSAL.NOMBRE SUCURSALNOMBRE,
  DOCTO.FOLIO ,
  DOCTO.FECHA,
  DOCTO.REFERENCIA FACTURA,
  DOCTO.FECHAFACTURA,
  DOCTO.TOTAL,
  DOCTO.SALDO,
  DOCTO.ABONO,
  CASE WHEN TIPODOCTOID = 1011 THEN DOCTO.ABONO ELSE 0.00 END AS MONTOAPLICAR,
  DOCTO.ORIGENFISCALID,
  ORIGENFISCAL.NOMBRE ORIGENFISCALNOMBRE,
DOCTO.ID DOCTOID,
  DOCTO.PERSONAID PROVEEDORID,
 PERSONA.NOMBRE PROVEEDORNOMBRE
  FROM DOCTO
  LEFT JOIN SUCURSAL ON DOCTO.SUCURSALTID = SUCURSAL.ID
  LEFT JOIN ORIGENFISCAL ON ORIGENFISCAL.ID = DOCTO.ORIGENFISCALID
 LEFT JOIN PERSONA ON PERSONA.ID = DOCTO.PERSONAID
  WHERE 

(0 = @FOLIO OR DOCTO.FOLIO = @FOLIO) AND 
(0 = @SUCURSALTID OR DOCTO.SUCURSALTID = @SUCURSALTID) AND 
(0 = @TIPODOCTOID OR DOCTO.TIPODOCTOID = @TIPODOCTOID) AND
(@CONSALDO &lt;&gt; 'S' OR DOCTO.SALDO &gt; 0) AND
(0 = @PROVEEDORID  OR DOCTO.PERSONAID = @PROVEEDORID)</CommandText>
