                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select pago.id, pago.fecha, pago.fechahora, pago.importe,
       formapago.clave formapagoclave, formapago.nombre formapagonombre,
       formapagosat.clave formapagosatclave,  formapagosat.nombre formapagosatnombre,
         pago.banco, pago.referenciabancaria,
       tipopago.clave tipopagoclave, tipopago.nombre tipopagonombre  ,
       bancos.nombre banconombre,
       case when coalesce(pago.reciboelectronicoid ,0) &lt;&gt; 0 then 'S' else 'N' end TIMBRADO,
      pago.aplicado,
      persona.clave personaclave,
      persona.nombre personanombre

       from pago
       left join formapago on formapago.id = pago.formapagoid
       left join formapagosat on formapagosat.id = pago.formapagosatid
       left join tipopago on tipopago.id = pago.tipopagoid
       left join bancos on pago.banco = bancos.id
       left join persona on persona.id = pago.personaid
       where 
pago.subtipopagoid = 2 and
pago.formapagoid &lt;&gt; 4 and pago.fecha &gt;= @fechainicial and pago.fecha &lt;= @fechafinal and pago.tipopagoid = 2 and (@incluircancelaciones = 'S' OR ( coalesce(pago.revertido,'N') = 'N' AND coalesce(pago.tipoabonoid,1) &lt;&gt; 3))
       and (pago.formapagoid = @formapagoid or @formapagoid = 0)
and (pago.personaid = @personaid or @personaid = 0)
order by pago.fechahora</CommandText>
