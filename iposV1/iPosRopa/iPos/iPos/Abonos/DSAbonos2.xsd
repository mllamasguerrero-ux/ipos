  ORIGENFISCAL.NOMBRE ORIGENFISCALNOMBRE,
DOCTO.ID DOCTOID,
DOCTO.IVA,
DOCTO.IEPS,
DOCTO.PERSONAID,

SUM(CASE WHEN COALESCE(MOVTO.TASAIEPS,0) = 8 THEN COALESCE(MOVTO.IEPS,0) ELSE 0 END) IEPS8,
SUM(CASE WHEN COALESCE(MOVTO.TASAIEPS,0) = 25 THEN COALESCE(MOVTO.IEPS,0) ELSE 0 END) IEPS25,
SUM(CASE WHEN COALESCE(MOVTO.TASAIEPS,0) = 26 THEN COALESCE(MOVTO.IEPS,0) ELSE 0 END) IEPS26,
SUM(CASE WHEN COALESCE(MOVTO.TASAIEPS,0) = 30 THEN COALESCE(MOVTO.IEPS,0) ELSE 0 END) IEPS30,
SUM(CASE WHEN COALESCE(MOVTO.TASAIEPS,0) = 53 THEN COALESCE(MOVTO.IEPS,0) ELSE 0 END) IEPS53,

DOCTO.SUBTOTAL



  FROM DOCTO
  LEFT JOIN SUCURSAL ON DOCTO.SUCURSALTID = SUCURSAL.ID
  LEFT JOIN ORIGENFISCAL ON ORIGENFISCAL.ID = DOCTO.ORIGENFISCALID
  LEFT JOIN MOVTO ON MOVTO.DOCTOID = DOCTO.ID
  WHERE DOCTO.FOLIO = @FOLIO AND DOCTO.SUCURSALTID = @SUCURSALTID AND DOCTO.TIPODOCTOID = 1011

