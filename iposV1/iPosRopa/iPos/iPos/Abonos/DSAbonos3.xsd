                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select  pago.id pagoid, doctopago.id doctopagoid, pago.importe importepago,
 pago.importerecibido importerecibidopago, pago.referenciabancaria referenciabancariapago,
 pago.fecha fechapago, cajero.clave cajeroclavepago,  cajero.nombre cajeronombrepago,
pago.revertido,
pago.formapagoid,
formapago.nombre formapagonombre,
 coalesce(cajero.nombres ,'') cajeronombres,
 coalesce(cajero.apellidos ,'') cajeroapellidos,
   
coalesce(tipodocto.id,  tipodoctosalida.id)  tipodoctoid ,
coalesce(tipodocto.nombre,  tipodoctosalida.nombre)  tipodoctonombre ,
coalesce(clientes.id,  proveedores.id)  personaid   ,
coalesce(clientes.nombre,  proveedores.nombre)  personanombre  ,     
coalesce(docto.id,  doctosalida.id)  doctoid  ,
coalesce(docto.folio,  doctosalida.folio)  doctofolio  ,  
coalesce(docto.fecha,  doctosalida.fecha)  doctofecha ,  
coalesce(docto.total,  doctosalida.total)  doctototal


from pago
left join doctopago on doctopago.pagoid = pago.id
left join corte on corte.id = pago.corteid
left join persona cajero on cajero.id = corte.cajeroid
left join docto on docto.id = doctopago.doctoid
left join tipodocto on tipodocto.id = docto.tipodoctoid
left join persona clientes on clientes.id = docto.personaid
left join docto doctosalida on doctosalida.id = doctopago.doctosalidaid
left join tipodocto tipodoctosalida on tipodoctosalida.id = doctosalida.tipodoctoid 
left join persona proveedores on proveedores.id = doctosalida.personaid
left join formapago on formapago.id = pago.formapagoid
where
coalesce(pago.referenciabancaria,'') &lt;&gt; '' and coalesce(pago.referenciabancaria,'') = @referenciabancaria
and coalesce(pago.banco,0) &lt;&gt; 0 and coalesce(pago.banco, 0) = @bancoid
and pago.importerecibido = @importerecibido 
and pago.formapagoid = @formapagoid</CommandText>
