                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        corte.id AS folio, sucursal.clave AS sucursal, caja.clave AS caja, turno.nombre AS turno, persona.nombre AS cajero, corte.numerotickets AS num_tickets, corte.numerodevoluciones AS num_devoluciones, corte.saldoinicial, 
                         corte.ingreso, corte.egreso, corte.devolucion, corte.aportacion, corte.retiro, corte.saldofinal, corte.saldoreal, corte.diferencia, corte.fechacorte, corte.inicia, corte.termina, corte.ingresoefectivo, corte.ingresotarjeta, 
                         corte.ingresocredito, corte.ingresocheque, corte.ingresovale, corte.cambiocheque, corte.pagoproveedores, corte.fondodinamico fondodinamicoinicial, corte.fondodinamicofinal fondodinamicofinal, coalesce(corte.fondodinamico , 0) -  coalesce(corte.fondodinamicofinal,0) fondodinamicodiferencia

FROM            corte LEFT OUTER JOIN
                         sucursal ON sucursal.id = corte.sucursalid LEFT OUTER JOIN
                         caja ON caja.id = corte.cajaid LEFT OUTER JOIN
                         turno ON turno.id = corte.turnoid LEFT OUTER JOIN
                         persona ON corte.cajeroid = persona.id
WHERE        (corte.id = @corteid) AND (corte.sucursalid =
                             (SELECT        sucursalid
                               FROM            parametro))</CommandText>
