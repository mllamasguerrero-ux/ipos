                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PPARTE.ID AS PRODUCTOID, PPARTE.CLAVE AS PRODUCTOCLAVE, PPARTE.NOMBRE AS PRODUCTONOMBRE, PPARTE.DESCRIPCION AS PRODUCTOdescripcion, 
                         PPARTE.EXISTENCIA, SUM((M.CANTIDAD - PKIT.EXISTENCIA) * K.CANTIDADPARTE) AS CANTIDADREQUERIDA, SUM((M.CANTIDAD - PKIT.EXISTENCIA) 
                         * K.CANTIDADPARTE) - PPARTE.EXISTENCIA AS FALTANTE
FROM            MOVTO M LEFT OUTER JOIN
                         DOCTO D ON D.ID = M.DOCTOID INNER JOIN
                         KITDEFINICION K ON K.PRODUCTOKITID = M.PRODUCTOID LEFT OUTER JOIN
                         PRODUCTO PKIT ON PKIT.ID = M.PRODUCTOID LEFT OUTER JOIN
                         PRODUCTO PPARTE ON PPARTE.ID = K.PRODUCTOPARTEID
WHERE        (M.DOCTOID = @DOCTOID) AND (M.CANTIDAD &gt; 0) AND (PKIT.ESKIT = 'S')
GROUP BY PPARTE.ID, PPARTE.CLAVE, PPARTE.NOMBRE, PPARTE.DESCRIPCION, PPARTE.EXISTENCIA
HAVING        (SUM((M.CANTIDAD - PKIT.EXISTENCIA) * K.CANTIDADPARTE) - PPARTE.EXISTENCIA &gt; 0)</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="DOCTOID" ColumnName="DOCTOID" DataSourceName="MOVTO" DataTypeServer="DbType.Int64(8)" DbType="Int32" Direction="Input" ParameterName="@DOCTOID" Precision="0" Scale="0" Size="8" SourceColumn="DOCTOID" SourceColumnNullMapping="false" SourceVersion="Current" />
