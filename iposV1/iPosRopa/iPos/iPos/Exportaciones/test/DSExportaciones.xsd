                      <Parameter AllowDbNull="true" AutogeneratedName="FECHATO" ColumnName="FECHA" DataSourceName="DOCTO" DataTypeServer="DbType.DateTime(4)" DbType="DateTime" Direction="Input" ParameterName="@FECHATO" Precision="0" Scale="0" Size="4" SourceColumn="FECHA" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="SOLONOENVIADOS" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="AnsiString" Direction="Input" ParameterName="@SOLONOENVIADOS" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
