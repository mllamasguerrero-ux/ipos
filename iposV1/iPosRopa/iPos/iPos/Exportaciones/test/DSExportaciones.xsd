              <DbSource ConnectionRef="ConnectionIpos (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        DOCTO.ID, DOCTO.FOLIO, SUBTIPODOCTO.NOMBRE, DOCTO.FECHA, DOCTO.FECHAINI, DOCTO.FECHAFIN, DOCTO.CORTEID, DOCTO.VENDEDORID, 
                         PERSONA.NOMBRE AS NOMBRECAJERO, 
                         CASE DOCTO.TRASLADOAFTP WHEN 'S' THEN 'ENVIADO' WHEN 'G' THEN 'EN PROCESO DE ENVIO' ELSE 'PENDIENTE' END AS ESTATUSPEDIDO, 
                         DOCTO.SUBTIPODOCTOID, DOCTO.TRASLADOAFTP, EXP_FILES.ID AS EXPFILEID, EXP_FILES.ESTATUS,
    SUCURSAL.CLAVE SUCURSALCLAVE, SUCURSAL.NOMBRE SUCURSALNOMBRE
FROM            DOCTO LEFT OUTER JOIN
                         SUBTIPODOCTO ON DOCTO.SUBTIPODOCTOID = SUBTIPODOCTO.ID LEFT OUTER JOIN
                         PERSONA ON DOCTO.VENDEDORID = PERSONA.ID LEFT OUTER JOIN
                         EXP_FILES ON EXP_FILES.DOCTOID = DOCTO.ID AND EXP_FILES.TIPO = 'P' LEFT OUTER JOIN
                        SUCURSAL ON SUCURSAL.ID = DOCTO.SUCURSALTID
WHERE        (DOCTO.FECHA &gt;= @FECHAFROM) AND (DOCTO.FECHA &lt;= @FECHATO)  AND DOCTO.TIPODOCTOID = 14 AND COALESCE(DOCTO.SUBTIPODOCTOID,0) = 29 AND 
(DOCTO.TRASLADOAFTP &lt;&gt; 'S' OR @SOLONOENVIADOS &lt;&gt; 'S')</CommandText>
