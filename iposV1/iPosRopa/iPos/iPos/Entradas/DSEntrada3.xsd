                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select tipo, fechahora, id, clave, nombre, descripcion, descripcion2, cantidadcomprada ,cajas, piezas, pzacaja
from
(
select
   'compra reciente'  tipo, max(docto.fecha) fecha, max(docto.fechahora) fechahora,
    producto.id, producto.clave, producto.nombre, producto.descripcion , producto.descripcion2,
   sum(coalesce(movto.cantidad,0)) cantidadcomprada ,
   trunc( sum(coalesce(movto.cantidad,0)) /
        case when coalesce(producto.pzacaja , 0.00) = 0.00 then 1.00 else producto.pzacaja end) cajas ,
    mod( sum(coalesce(movto.cantidad,0)) ,
        case when coalesce(producto.pzacaja , 0.00) = 0.00 then 1.00 else producto.pzacaja end ) piezas,
        producto.pzacaja

   from
   docto
   inner join movto on movto.doctoid = docto.id
   inner join producto on producto.id = movto.productoid

   where docto.tipodoctoid in (11,41) and docto.fecha &gt;= @fechaminima and docto.fecha &lt;= @fechamaxima and
         docto.fechahora &gt;= @fechahoraminima and docto.fechahora &lt;= @fechahoramaxima
    group by producto.id, producto.clave, producto.nombre, producto.descripcion , producto.descripcion2 ,producto.pzacaja


    union

 select
    'nuevo producto' tipo,
    cast(producto.creado as date) fecha,
    producto.creado fechahora,
    producto.id, producto.clave, producto.nombre, producto.descripcion , producto.descripcion2,
    0.00   cantidadcomprada,
    0.00 cajas,
    0.00 piezas,
        producto.pzacaja
    from
     producto
       where cast(producto.creado as date) &gt;= @fechaminima  and cast(producto.creado as date) &lt;= @fechamaxima
  ) p

  order by fechahora
</CommandText>
