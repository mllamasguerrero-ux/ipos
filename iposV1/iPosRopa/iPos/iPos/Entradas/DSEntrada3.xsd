                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
FO.ID, FO.FACTURA, FO.FECHARECEPCION, FO.FECHAFACTURA, PROVEEDOR.CLAVE PROVEEDORCLAVE,
PROVEEDOR.NOMBRE PROVEEDORNOMBRE, FO.SUMA, FO.IVA, FO.IEPS8, FO.IEPS26, FO.IEPS26C, FO.IEPS30, FO.IEPS53,
FO.TOTAL, FO.PROVISIONADA,
FO.COMPRAFOLIO, FO.SUCURSALID, SUCURSAL.NOMBRE SUCURSALNOMBRE, SUCURSAL.CLAVE SUCURSALCLAVE
FROM FACTURAORIGINALCOMPRA FO
LEFT JOIN PERSONA PROVEEDOR ON PROVEEDOR.ID = FO.PROVEEDORID
LEFT JOIN SUCURSAL ON SUCURSAL.ID = FO.SUCURSALID
WHERE

((fo.FECHAFACTURA &gt;= @fechainicial and fo.FECHAFACTURA &lt;= @fechafinal ) or @porfechafact &lt;&gt; 'S') 
and ((fo.FECHARECEPCION &gt;= @fecharecinicial and fo.FECHARECEPCION&lt;= @fecharecfinal ) or @porfecharec &lt;&gt; 'S')
 and (@incluirprovisionados = 'S' or fo.provisionada = 'N')
and (fo.PROVEEDORID = @personaid or @personaid = 0)
and (fo.SUCURSALID = @sucursalid or @sucursalid = 0 )
order by fo.fechafactura</CommandText>
